<h1> Welcome New Artist!</h1> 
<h3>Please Register so we may get you started</h3>

<% if flash[:errors] %>
<% flash[:errors].each do |error|%>
<li><%= error %></li>
<% end %>
<% end %>

<%= form_for @artist do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :age %>
<%= f.number_field :age %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.submit %>
<% end %>

